  "tokenColors": [
    {
      "scope": "source | text",
      "settings": {
        "foreground": "#cacde2"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "foreground": "#4e5c7e"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#cacde2"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#fb7185"
      }
    },
    {
      "name": "Invalid & deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#ff9c66"
      }
    },
    {
      "name": "Keyword, directive, import, storage type",
      "scope": [
        "keyword",
        "keyword.directive",
        "keyword.control.directive",
        "keyword.other.import",
        "keyword.control.import",
        "keyword.control.at-rule.import",
        "meta.preprocessor",
        "keyword.other",
        "keyword.other.unit",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.word",
        "storage.type"
      ],
      "settings": {
        "foreground": "#c792ea"
      }
    },
    {
      "name": "Storage modifier",
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#82f1b1"
      }
    },
    {
      "name": "Control keyword",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#67e8f9"
      }
    },
    {
        "name": "Operator",
        "scope": "keyword.operator",
        "settings": {
            "foreground": "#8da1ee"
        }
    },
    {
      "name": "Punctuation",
      "scope": [
        "keyword.operator.type",
        "keyword.operator.optional",
        "punctuation.accessor",
        "punctuation.definition",
        "punctuation.other",
        "punctuation.separator",
        "punctuation.separator.key-value",
        "punctuation.separator.label",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#6d88a7"
      }
    },
    {
      "name": "Brace",
      "scope": [
        "punctuation.definition.arguments",
        "punctuation.definition.tag",
        "punctuation.definition.parameters",
        "punctuation.definition.string",
        "punctuation.definition.variable",
        "punctuation.support",
        "punctuation.support.type",
        "punctuation.section.bracket",
        "punctuation.section.brackets",
        "punctuation.section.group",
        "punctuation.section.groups",
        "punctuation.section.round",
        "punctuation.section.arguments",
        "punctuation.section.parens"
      ],
      "settings": {
        "foreground": "#d2d8e7"
      }
    },
    {
      "name": "Section",
      "scope": [
        "markup.heading",
        "punctuation.section",
        "punctuation.section.block",
        "punctuation.section.brace",
        "punctuation.section.braces",
        "punctuation.section.function"
      ],
      "settings": {
          "foreground": "#6d88a7"
      }
    },
    {
      "name": "Tag",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#c792ea"
      }
    },
    {
      "name": "Header",
      "scope": [
        "markup.heading entity.name",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#54bef2"
      }
    },
    {
      "name": "Function, special method, markup change",
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function",
        "entity.name.method.js",
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor",
        "keyword.other.special-method",
				"support.constant.handlebars",
				"source.powershell variable.other.member",
				"entity.name.operator.custom-literal"
      ],
      "settings": {
        "foreground": "#54bef2"
      }
    },
    {
      "name": "Constant variable",
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#fb7185"
      }
    },
    {
      "name": "Support variable",
      "scope": "support.other.variable",
      "settings": {
        "foreground": "#fb7185"
      }
    },
    {
      "name": "Constant, tag attribute, embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape",
        "support.constant"
      ],
      "settings": {
        "foreground": "#fb7185"
      }
    },
    {
      "name": "Function argument",
      "scope": [
        "variable.parameter",
        "variable.parameter.function.language.special"
      ],
      "settings": {
        "foreground": "#fb7185"
      }
    },
    {
      "name": "String, symbol",
      "scope": [
        "string",
        "string.other.link",
        "constant.other.symbol",
        "constant.other.key",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#c3e88d"
      }
    },
    {
      "name": "Class, support",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "entity.name.scope-resolution",
        "entity.other.inherited-class",
				"entity.other.attribute",
        "support.class",
        "support.type",
        "support.type.exception"
      ],
      "settings": {
        "foreground": "#ffcb6b"
      }
    },
    {
      "name": "Entity type",
      "scope": "support.type",
      "settings": {
        "foreground": "#d2d8e7"
      }
    },
    {
      "name": "Submodule",
      "scope": [
        "entity.name.namespace",
        "variable.import.parameter",
        "variable.other.class"
      ],
      "settings": {
        "foreground": "#ffcb6b"
      }
    },
    {
      "name": "Language variable",
      "scope": [
        "entity.name.module",
        "variable.language"
      ],
      "settings": {
        "foreground": "#fb7185"
      }
    },
    {
      "name": "Property",
      "scope": [
        "variable.object.property",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#d2d8e7"
      }
    },
    {
      "name": "Label",
      "scope": "entity.name.label",
      "settings": {
        "foreground": "#d2d8e7"
      }
    },
    {
      "name": "Module namespace",
      "scope": "entity.name.type.module",
      "settings": {
        "foreground": "#cacde2"
      }
    },
    {
      "name": "Wildcard",
      "scope": [
        "keyword.operator.glob",
        "keyword.operator.wildcard",
        "variable.language.wildcard"
      ],
      "settings": {
        "foreground": "#ffcb6b"
      }
    },
    {
      "name": "Decorator, annotation",
      "scope": [
        "entity.name.tag.js",
        "punctuation.definition.tag.js",
        "storage.type.annotation",
        "punctuation.decorator",
        "meta.decorator variable.other.readwrite",
        "meta.decorator entity.name.function"
      ],
      "settings": {
        "foreground": "#ff9c66"
      }
    },
    {
      "name": "Attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#54bef2"
      }
    },
    {
      "name": "Regular expression",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#67e8f9"
      }
    },
    {
      "name": "Regular expression delimiter",
      "scope": "string.regexp punctuation.definition.string",
      "settings": {
        "foreground": "#d2d8e7"
      }
    },
    {
      "name": "Escape character",
      "scope": [
        "constant.character.escape",
        "constant.other.character-class"
      ],
      "settings": {
        "foreground": "#fb7185"
      }
    },
    {
      "name": "Regular expression anchor",
      "scope": [
        "keyword.control.anchor.regexp"
      ],
      "settings": {
        "foreground": "#c792ea"
      }
    },
    {
      "name": "Regular expression assertion",
      "scope": [
        "string.regexp keyword.other.operator",
        "string.regexp meta.assertion"
      ],
      "settings": {
        "foreground": "#54bef2"
      }
    },
    {
      "name": "Regular expression alternation",
      "scope": "keyword.operator.or.regexp",
      "settings": {
        "foreground": "#d2d8e7"
      }
    },
    {
      "name": "Regular expression class/group",
      "scope": [
        "string.regexp punctuation.definition.character-class",
        "string.regexp punctuation.definition.group"
      ],
      "settings": {
        "foreground": "#cacde2"
      }
    },
    {
      "name": "Documentation tag",
      "scope": "punctuation.definition.block.tag",
      "settings": {
        "foreground": "#d2d8e7"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Link description/title",
      "scope": [
        "constant.other.reference.link",
        "string.other.link.description",
        "string.other.link.title"
      ],
      "settings": {
        "foreground": "#c3e88d"
      }
    },
    {
      "name": "[Markup] Changed",
      "scope": ["markup.changed", "markup.changed.git_gutter"],
      "settings": {
        "foreground": "#ffcb6b"
      }
    },
    {
      "name": "[Markup] Deleted",
      "scope": [
        "markup.deleted",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#fb7185"
      }
    },
    {
      "name": "[Markup] Insertion",
      "scope": [
        "markup.inserted",
        "markup.inserted.git_gutter"
      ],
      "settings": {
        "foreground": "#82f1b1"
      }
    },
    {
      "name": "[Markup] Bold",
      "scope": ["markup.bold", "markup.bold string"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "[Markup] Italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "[Markup] Bold-italic",
